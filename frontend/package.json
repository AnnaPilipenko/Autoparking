{
  "name": "autoparking",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "font-awesome": "^4.7.0",
    "rc-datepicker": "^5.0.13",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-fontawesome": "^1.6.1",
    "react-scripts": "1.1.4",
    "react-select": "^1.3.0",
